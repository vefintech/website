<% if ((associate.type=='ENTREPENEUR')) {%>
<div class="row" id="entrepreneur-div" >
<div class="col-xl-12">
<form id="entrepreneur-form" enctype="multipart/form-data" method="post" action="/admin/asociados/actualizar" data-parsley-validate="" novalidate="">
    <!-- START card-->
    <div class="card card-default">

    <div class="card-body">
        <div class="form-group">
            <label class="col-form-label" for="names">Nombres y Apellidos *</label>
            <input value="<%= associate?associate.names: ''%>" placeholder="Nombres y Apellidos" class="form-control" id="names" type="text" name="names"  data-parsley-minlength="10" required>
        </div>
        <% if ((associate)) {%>
            <div class="px-2">
            <img onerror="this.onerror=null;this.src='/img/user-default.png'" class="img-fluid rounded thumb64" src="/img/associates/<%= associate.image %>" alt="">
            </div>  
        <% }%>
                                      
        <div class="form-group">
            <label class="col-form-label" for="image">Logo</label>
            <input <%= (associate)?'':'required'%> accept="image/png, image/jpeg" class="form-control-file" id="image" type="file" name="image"  %> >
        </div>  
        <div class="form-group">
            <label class="col-form-label" for="city">Ciudad *</label>
            <input value="<%= associate?associate.city: ''%>"  placeholder="Ciudad" class="form-control" id="city" type="text" name="city"  %> required>
        </div>
        <div class="form-group">
            <label class="col-form-label" for="nationality">Nacionalidad *</label>
            <input value="<%= associate?associate.nationality: ''%>"  placeholder="Nacionalidad" class="form-control" id="nationality" type="text" name="nationality"  %> required>
        </div>
        <div class="form-group">
            <label class="col-form-label" for="phone">Telefono</label>
            <input value="<%= associate?associate.phone: ''%>"  placeholder="Telefono" class="form-control" id="phone" type="phone" name="phone"  %> data-parsley-minlength="11">
        </div>  
        <div class="form-group">
            <label class="col-form-label" for="entrepreneurship">Nombre del emprendimiento</label>
            <input required value="<%= associate?associate.entrepreneurship: ''%>"  placeholder="Nombre del emprendimiento" class="form-control" id="entrepreneurship" type="text" name="entrepreneurship"  %> >
        </div> 
        <div class="form-group">
            <label class="col-form-label" for="phase">Fase del proyecto *</label>
            <div class="form-check">
                <input <%= associate?associate.phase=='Semilla'?'checked': '': 'checked'%> class="form-check-input" type="radio" name="phase" id="phase_1" value="Semilla">
                <label class="form-check-label" for="phase_1">
                Semilla
                </label>
            </div>  
            <div class="form-check">
                <input <%= associate?associate.phase=='Pruebas'?'checked': '': ''%> class="form-check-input" type="radio" name="phase" id="phase_2" value="Pruebas">
                <label class="form-check-label" for="phase_2">
                Pruebas
                </label>
            </div>
            <div class="form-check">
                <input <%= associate?associate.phase=='En Marcha'?'checked': '': ''%> class="form-check-input" type="radio" name="phase" id="phase_3" value="En Marcha">
                <label class="form-check-label" for="phase_3">
                En Marcha
                </label>
            </div> 
        </div>

        <div class="form-group">
            <label class="col-form-label" for="services">Servicios que ofreces *</label>
            <textarea required   placeholder="Servicios que ofreces" class="form-control" id="services"  name="services"  %> ><%= associate?associate.services: ''%></textarea>
        </div>   
        
        <input class="form-control" name="type" value="ENTREPENEUR" type="hidden">
        <input class="form-control" value="" type="hidden" name="bot">
        <div class="required">* Campos requeridos</div>
    </div>
    <div class="card-footer">
        <div class="clearfix">
            <div class="float-left">

            </div>
            <div class="float-right">
                <button class="btn btn-primary" type="submit">Guardar</button>
            </div>
        </div>
    </div>
    </div>
    <!-- END card-->
</form>
</div>
</div>
<% }%>